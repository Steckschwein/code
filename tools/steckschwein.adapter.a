!src <defs.h.a>
!src <via.h.a>
!src <spi.a>

.io_init
!src "../bios/vdp.a"
   
.chrout
    pha
    phx
    jsr vdp_chrout
    plx
    pla
    rts 
	
.getkey
	phx
	lda #%01111010
	sta via1portb
	jsr .spi_r_byte
	ldx #%11111110
	stx via1portb
	plx
    cmp #$00; restore zero flag
    rts 
