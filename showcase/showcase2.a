*=$c000
	
main
	jsr $1000

	jmp main

irq
!for i,256 {
	cmp #$c9
}
	rti
reset
	ldx #$ff
	txs
	
	cld

	cli
	
	jsr nopslide

	jmp main

nopslide
	pha
	phy

	lda #$ea
	ldy #$00

-
	sta $1000,y
	iny
	cpy #$ff
	bne -

	lda #$60
	sta $1100
	
	pla
	ply
	rts
	

;----------------------------------------------------------------------------------------------
; Interrupt vectors
;----------------------------------------------------------------------------------------------
; $FFFA/$FFFB NMI Vector
*= $fffa
!byte <main
!byte >main
; $FFFC/$FFFD reset vector
;*= $fffc
!byte <main
!byte >main
; $FFFE/$FFFF IRQ vector
;*= $fffe
!byte <irq
!byte >irq
