*=$0400
!src <defs.h.a>
!src <bios.h.a>

	lda #dev_lcd
	sta chn_out 

	jsr lcdclear
	+PrintString msg
-
a	lda biosdata
b	sta $e000

	inc a+1
	inc b+1
	bne -

	inc a+2
	inc b+2
	bne -

done
	+PrintString msgdone
	
	lda #$01
	sta $02f0
	
	jmp ($fffc)
	
-	jmp -	

msg	!text "Loading.. ",$00
msgdone	!text "done.",$00


*=$0900
biosdata
!bin "bios.bin"
