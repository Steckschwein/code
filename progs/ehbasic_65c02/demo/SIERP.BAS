100 SCREEN 7
120 DEF FN M(X) = X - INT(D*3)*INT(X/INT(D*3))
130 DE=4
140 DI=3^DE*3
150 FOR I=0 TO DI-1
160 FOR J=0 TO DI-1
170 FOR D=INT(DI/3) TO 0 STEP 0
180 IF INT(FN M(I) / D) = 1 AND INT(FN M(J) / D) =1 THEN 200
190 D = INT(D/3): NEXT D
200 CO=3*((D=0) AND 1)
210 PLOT J,I,CO
220 NEXT J
230 NEXT I
